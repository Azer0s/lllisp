;; Basic function example for LLLisp
(def stdio (use :header "stdio.h"))

;; Define a function that adds two numbers
(def add (fn [(:a i32) (:b i32)] i32
  (+ a b)
))

;; Main function that uses the add function
(def main (fn [] i32
  (do
    (printf "Testing function call...\n")
    
    ;; Call the add function
    (def result (add 7 3))
    
    ;; Print the result
    (printf "7 + 3 = %d\n" result)
    
    0
  )
)) 